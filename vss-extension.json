{
  "manifestVersion": 1,
  "id": "SupOlivier-querytile-extended",
  "version": "0.0.3",
  "name": "Query Tile Extended",
  "description": "Query Tile Widget, support all query type, with functions: sum, highest, lowest, average, ect. , on all integer or decimal values of your WI, with rounding and multiplication factor.",
  "publisher": "SupOlivier",
  "scopes": [
    "vso.work"
  ],
  "galleryFlags": [
    "Public"
  ],
  "categories": [
    "Collaborate"
  ],
  "tags": [
    "dashboard",
    "widget",
    "query",
    "tile",
    "aggregation",
    "sum",
    "count",
    "average",
    "days",
    "point"
  ],
  "links": {
    "getstarted": {
      "uri": "https://github.com/SupOlivier/querytile-extended"
    },
    "support": {
      "uri": "https://github.com/SupOlivier/querytile-extended/issues"
    }
  },
  "content": {
    "details": {
        "path": "overview.md"
    },
    "license": {
        "path": "license.md"
    }
},
  "repository": {
    "type": "git",
    "uri": "https://github.com/SupOlivier/querytile-extended"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "demands": [
    "contribution/ms.vss-dashboards-web.widget-sdk-version-2"
  ],
  "contributions": [
    {
      "id": "querytile-extended-widget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".querytile-extended-widget-configuration"
      ],
      "properties": {
        "name": "Query Tile Extended",
        "description": "Query Tile Widget, support all query type, with aggregation functions: sum, highest, lowest, average, count of empty or non empty values, on all integer or decimal values of your WI, with rounding and multiplication factor.",
        "catalogIconUrl:": "images/logo.png",
        "previewImageUrl": "images/logo.png",
        "uri": "widget.html",
        "isNameConfigurable": true,
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 1
          },
          {
            "rowSpan": 1,
            "columnSpan": 2
          },
          {
            "rowSpan": 1,
            "columnSpan": 3
          },
          {
            "rowSpan": 2,
            "columnSpan": 2
          },
          {
            "rowSpan": 2,
            "columnSpan": 3
          }
        ],
        "supportedScopes": [
          "project_team"
        ]
      }
    },
    {
      "id": "querytile-extended-widget-configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [
        "ms.vss-dashboards-web.widget-configuration"
      ],
      "properties": {
        "name": "Query Tile Extended Configuration",
        "description": "Query Tile Widget, support all query type, with functions: sum, highest, lowest, average, ect. , on all integer or decimal values of your WI, with rounding and multiplication factor.",
        "uri": "configuration.html"
      }
    }
  ],
  "files": [
    {
      "path": "widget.html",
      "addressable": true
    },
    {
      "path": "configuration.html",
      "addressable": true
    },
    {
      "path": "scripts/VSS.SDK.min.js",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "scripts/array-polyfill.js",
      "addressable": true
    }
  ],
  "screenshots": [
    {
      "path": "images/preview.png"
    },
    {
      "path": "images/preview_config.png"
    }
  ]
}